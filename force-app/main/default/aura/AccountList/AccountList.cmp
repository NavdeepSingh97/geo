<aura:component controller="AccountController">
<aura:attribute name="account" type="Account[]"/>
<aura:handler name="init" value="{!this}" action="{!c.doinit}"/>
<ul>
<aura:iteration value="{!v.account}" var="acc" >
    <c:AccountListItem account="{!acc}"/>
</aura:iteration>

</ul>

</aura:component>	
